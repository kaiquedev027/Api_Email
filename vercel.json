{
    "version": 2,
    "builds": [
        {
            "src": "/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "index.js"
        }
    ],
    "functions": {
        "api/send.js": {
            "memory": 512,
            "maxDuration": 10
        }
    }
}